{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd app/ui/console && npm ci && npm run build",
  "outputDirectory": "app/ui/console/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/:path((?!api).*)*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
